{
  "name": "@biosimulations/nestjs-bullmq",
  "version": "1.0.7",
  "private": false,
  "description": "Nest - modern, fast, powerful node.js web framework (@bullmq)",
  "homepage": "https://github.com/biosimulations/nestjs-bullmq",
  "bugs": {
    "url": "https://github.com/biosimulations/nestjs-bullmq/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/biosimulations/nestjs-bullmq.git"
  },
  "license": "MIT",
  "contributors": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "npm run build",
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "format": "prettier -c **/*.{md,ts,json,yml}",
    "format:fix": "prettier --write **/*.{md,ts,json,yml}",
    "lint": "eslint \"lib/**/*.ts\"",
    "lint:fix": "eslint \"lib/**/*.ts\" --fix",
    "release": "semantic-release",
    "test": "jest --detectOpenHandles --forceExit --coverage",
    "test:e2e": "jest --config=jest-e2e.config.js --detectOpenHandles --forceExit --coverage --runInBand",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,md,json}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@nestjs/common": "^8.2.5",
    "@nestjs/core": "^8.2.5",
    "@nestjs/platform-express": "^8.2.5",
    "@nestjs/testing": "^8.2.5",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.3",
    "@types/reflect-metadata": "0.1.0",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "bullmq": "^1.32.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "hosted-git-info": ">=2.8.9",
    "husky": "^7.0.2",
    "jest": "26.6.3",
    "lint-staged": "11.1.2",
    "minimist": ">=1.2.3",
    "prettier": "^2.3.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "semantic-release": "^19.0.2",
    "sort-package-json": "1.50.0",
    "trim-newlines": ">=3.0.1",
    "ts-jest": "26.5.6",
    "ts-node": "10.0.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "4.4.2",
    "ws": ">=7.4.6",
    "y18n": ">=4.0.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.10.11 || ^7.0.0 || ^8.0.0",
    "@nestjs/core": "^6.10.11 || ^7.0.0 || ^8.0.0",
    "bullmq": "^1.36.0"
  }
}
